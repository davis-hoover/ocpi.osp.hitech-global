<!-- base config ALWAYS has rfdc to mitigate some weird constraints issues -->
<hdlconfig>
  <!-- if instancing rfdc, instance all rfdc* so software can reach all registers -->
  <device name='rfdc'/>
  <!-- TODO - investigate having a single rfdc_adc_or_dac_config.hdl that is instanced 8 times instead of separating adc and dac and 0-3 -->
  <device name='rfdc_dac_config_0'/>
  <device name='rfdc_dac_config_1'/>
  <device name='rfdc_dac_config_2'/>
  <device name='rfdc_dac_config_3'/>
  <device name='rfdc_adc_config_0'/>
  <device name='rfdc_adc_config_1'/>
  <device name='rfdc_adc_config_2'/>
  <device name='rfdc_adc_config_3'/>
</hdlconfig>
